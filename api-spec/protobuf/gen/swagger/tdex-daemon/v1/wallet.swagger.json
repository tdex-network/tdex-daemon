{
  "swagger": "2.0",
  "info": {
    "title": "tdex-daemon/v1/wallet.proto",
    "version": "version not set"
  },
  "tags": [
    {
      "name": "Wallet"
    }
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {},
  "definitions": {
    "BalanceInfo": {
      "type": "object",
      "properties": {
        "totalBalance": {
          "type": "string",
          "format": "uint64",
          "title": "The balance of the wallet"
        },
        "confirmedBalance": {
          "type": "string",
          "format": "uint64",
          "title": "The confirmed balance of a wallet(with \u003e= 1 confirmations)"
        },
        "unconfirmedBalance": {
          "type": "string",
          "format": "uint64",
          "title": "The unconfirmed balance of a wallet(with 0 confirmations)"
        }
      }
    },
    "SendToManyReply": {
      "type": "object",
      "properties": {
        "rawTx": {
          "type": "string",
          "format": "byte",
          "description": "The serialized transaction sent out on the network."
        },
        "txid": {
          "type": "string",
          "format": "byte",
          "description": "The id of the broadcasted transaction."
        }
      }
    },
    "TxOut": {
      "type": "object",
      "properties": {
        "asset": {
          "type": "string",
          "title": "The asset being spent"
        },
        "value": {
          "type": "string",
          "format": "int64",
          "description": "The value of the output being spent."
        },
        "address": {
          "type": "string",
          "description": "The confidential address of the output being spent."
        }
      }
    },
    "WalletAddressReply": {
      "type": "object",
      "properties": {
        "address": {
          "type": "string",
          "description": "The confidential address encoded using a blech32 format."
        },
        "blinding": {
          "type": "string",
          "title": "The blinding private key for the given address encoded in hex format"
        }
      }
    },
    "WalletBalanceReply": {
      "type": "object",
      "properties": {
        "balance": {
          "type": "object",
          "additionalProperties": {
            "$ref": "#/definitions/BalanceInfo"
          },
          "title": "The balance info (total, confirmed, unconfirmed) of the wallet grouped by\nasset"
        }
      }
    },
    "googlerpcStatus": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        },
        "details": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/protobufAny"
          }
        }
      }
    },
    "protobufAny": {
      "type": "object",
      "properties": {
        "@type": {
          "type": "string"
        }
      },
      "additionalProperties": {}
    }
  }
}
